{
  "compilerOptions": {
    "target": "es2016",
    "outDir": "dist",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "lib": ["ESNext"],
    "sourceMap": true,
    "strictPropertyInitialization": false,
    "baseUrl": "src",
    "paths": {
      "@/*": [".src/*"]
    },
    "moduleResolution": "node"
  }
}
